extends ../base.pug
include ../mixins/video

block content
    h1 Who is agungppang? #{agungppang}
    form(method="GET").search__form
        input(placeholder="Search by title", type="text", name="keyword")
        button
            i.fas.fa-search
    div.video-grid
    each video in videos
        +video(video)
    else
        span.empty__message No videos found